{

  "manifest_version": 2,
  "name": "Kahoot AI",
  "version": "1.0.0",

  "description": "An extension that uses machine learning to figure out the most likely answer for the kahoot question ",

  "host_permissions": [
    "*://serpapi.com/*"
  ],

  "content_scripts": [
    {
      "matches": ["*://kahoot.it/*", "*://*.kahoot.it/*"],
      "css": ["libs/toastr.min.css"],
      "js": ["libs/jquery.min.js", "libs/toastr.min.js", "index.js"]
    }
  ]

}
